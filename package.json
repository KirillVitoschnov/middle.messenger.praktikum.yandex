    "@types/mocha": "^10.0.10",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "chai": "^5.2.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.0.11",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.5.0",
    "mocha": "^11.1.0",
    "node-esm-loader": "^0.3.0",
    "postcss": "^8.4.45",
    "postcss-nesting": "^13.0.0",
